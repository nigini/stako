{
  "manifest_version": 3,
  "name": "STAKO",
  "version": "2.0.0",
  "description": "STAKO stores your Stack Overflow visits, provides personalized statistics, and adds to your experience of Stack Overflow Q&A pages.",
  "key": "",
  "icons": {
    "16": "img/stako16.png",
    "48": "img/stako48.png",
    "128": "img/stako128.png"
  },
  "action": {
    "default_icon": {
      "16": "img/stako16.png",
      "48": "img/stako48.png",
      "128": "img/stako128.png"
    },
    "default_popup": "html/popup.html",
    "default_title": "STAKO"
  },
  "background": {
    "service_worker": "js/bg-loader.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "identity",
    "identity.email"
  ],
  "host_permissions": [
    "https://stako.org/*",
    "https://stackoverflow.com/*"
  ],
  "oauth2": {
    "client_id": "504661212570-gn5nc6mtccuj7hsjcn419vjfapbq4f9a.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.email"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.stackoverflow.com/questions/*"],
      "css": ["css/design.css"],
      "js": ["js/jquery-3.5.1.min.js", "js/design.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}